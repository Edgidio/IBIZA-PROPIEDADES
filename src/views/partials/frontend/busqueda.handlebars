    <div class="site-section site-section-sm pb-0">
      <div class="container">
        <div class="row">
          <form action="/busqueda/propiedades" method="post"  class="form-search col-md-12" style="margin-top: -100px;">
            <div class="row  align-items-end">
              <div class="col-md-3">
                <label for="inmueble">TIPO DE INMUEBLE</label>
                <div class="select-wrap">
                  <span class="icon icon-arrow_drop_down"></span>
                  <select name="inmueble" id="inmueble" class="form-control d-block rounded-0">
                    <option value="Apartamento">Apartamento</option>
                    <option value="Casa">Casa</option>
                    <option value="Terreno">Terreno</option>
                    <option value="Local comercial">Local comercial</option>
                    <option value="Oficina">Oficina</option>
                    <option value="Edificio">Edificio</option>
                  </select>
                </div>
              </div>
              <div class="col-md-3">
                <label for="venta_renta">TIPO DE OFERTA</label>
                <div class="select-wrap">
                  <span class="icon icon-arrow_drop_down"></span>
                  <select name="venta_renta" id="venta_renta" class="form-control d-block rounded-0">
                    <option value="V">En venta</option>
                    <option value="R">En renta</option>
                  </select>
                </div>
              </div>
              <div class="col-md-3">
                <label for="estado">ESTADO</label>
                <div class="select-wrap">
                  <span class="icon icon-arrow_drop_down"></span>
                  <select name="estado" id="estado" class="form-control d-block rounded-0">
                    <option value="Amazonas">Amazonas</option>
                    <option value="Anzoátegui">Anzoátegui</option>
                    <option value="Apure">Apure</option>
                    <option value="Aragua">Aragua</option>
                    <option value="Barinas">Barinas</option>
                    <option value="Bolívar">Bolívar</option>
                    <option value="Carabobo">Carabobo</option>
                    <option value="Cojedes">Cojedes</option>
                    <option value="Delta Amacuro">Delta Amacuro</option>
                    <option value="Falcón">Falcón</option>
                    <option value="Guárico">Guárico</option>
                    <option value="Lara">Lara</option>
                    <option value="Mérida">Mérida</option>
                    <option value="Miranda">Miranda</option>
                    <option value="Monagas">Monagas</option>
                    <option value="Nueva Esparta">Nueva Esparta</option>
                    <option value="Portuguesa">Portuguesa</option>
                    <option value="Sucre">Sucre</option>
                    <option value="Táchira">Táchira</option>
                    <option value="Trujillo">Trujillo</option>
                    <option value="Vargas">Vargas</option>
                    <option value="Yaracuy">Yaracuy</option>
                    <option value="Zulia">Zulia</option>
                  </select>
                </div>
              </div>
              <div class="col-md-3">
                <input type="submit" class="btn  text-white btn-block rounded-0" value="Buscar">
              </div>
            </div>
          </form>
        </div>  


       
      </div>
    </div>

    <div class="site-section site-section-sm bg-light">
      <div class="container">

      {{#if propiedadesConUnaRutaPorFoto}}
        <div class="row mb-5">
          {{#each propiedadesConUnaRutaPorFoto}}
          <div class="col-md-6 col-lg-4 mb-4">
            <div class="property-entry h-100">
              <a href="/propiedad/{{id}}" class="property-thumbnail">
                <div class="offer-type-wrap">
                  {{#eq venta_renta "V"}}<span class="offer-type bg-amarillo">Venta</span>{{/eq}}
                  {{#eq venta_renta "R"}}<span class="offer-type bg-success">Renta</span>{{/eq}}
                </div>

                    <img loading="lazy" src="/images/uploads/propiedades/medium/{{rutas}}" alt="Image" class="img-fluid">
            
              </a>
              <div class="p-4 property-body">
                <a href="#" class="property-favorite"><span class="icon-heart-o"></span></a>
                <h2 class="property-title"><a href="#">{{descripcion}}</a></h2>
                <span class="property-location d-block mb-3"><span class="property-icon icon-room"></span>{{estado}}</span>
                <strong class="property-price text-primary mb-3 d-block text-success">{{formatNumber precio}}</strong>
                <ul class="property-specs-wrap mb-3 mb-lg-0">
                  <li>
                    <span class="property-specs">Habitaciones</span>
                    <span class="property-specs-number">{{n_habitaciones}} <sup>+</sup></span>
                    
                  </li>
                  <li>
                    <span class="property-specs">Baños</span>
                    <span class="property-specs-number">{{n_banos}}</span>
                    
                  </li>
                  <li>
                    <span class="property-specs">Metros Cuadrados</span>
                    <span class="property-specs-number">{{superficie}}</span>
                    
                  </li>
                </ul>

              </div>
            </div>
          </div>
          {{/each}}
        

        </div>

