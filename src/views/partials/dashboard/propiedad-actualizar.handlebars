<script>
    document.addEventListener("DOMContentLoaded", function() {
        // Obtener referencias a los campos a deshabilitar
        const nHabitacionesInput = document.getElementById("n_habitaciones");
        const nBanosInput = document.getElementById("n_banos");
        const superficieInput = document.getElementById("superficie");
        const terrenoInput = document.getElementById("terreno");
        const maleteroInput = document.getElementById("maletero");
        const estacionamientoInput = document.getElementById("estacionamiento");

        // Obtener referencia a los contenedores (div) de los campos
        const maleteroDiv = document.getElementById("campo-maletero");
        const estacionamientoDiv = document.getElementById("campo-estacionamiento");
        const habitacionesDiv = document.getElementById("campo-n_habitaciones");
        const banosDiv = document.getElementById("campo-n_banos");
        const superficieDiv = document.getElementById("campo-superficie");
        const terrenoDiv = document.getElementById("campo-terreno");

        // Obtener referencia al elemento de tipo de propiedad
        const tipoPropiedadSelect = document.getElementById("tipo_propiedad");

        // Función para deshabilitar y ocultar campos según el tipo de propiedad seleccionado
        function deshabilitarCampos() {
            const tipoSeleccionado = tipoPropiedadSelect.value;

            // Restablecer todos los campos y mostrarlos
            [nHabitacionesInput, nBanosInput, superficieInput, terrenoInput, maleteroInput, estacionamientoInput].forEach(input => input.disabled = false);
            [maleteroDiv, estacionamientoDiv, habitacionesDiv, banosDiv, superficieDiv, terrenoDiv].forEach(div => div.style.display = "");

            // Condiciones específicas para ocultar y deshabilitar campos según el tipo de propiedad
            if (tipoSeleccionado === "T") { // Terreno
                nHabitacionesInput.disabled = true;
                nBanosInput.disabled = true;
                superficieInput.disabled = true;
                habitacionesDiv.style.display = "none";
                banosDiv.style.display = "none";
                superficieDiv.style.display = "none";
            } else if (tipoSeleccionado === "O") { // Oficina
                nHabitacionesInput.disabled = true;
                terrenoInput.disabled = true;
                habitacionesDiv.style.display = "none";
                terrenoDiv.style.display = "none";
            } else if (tipoSeleccionado === "L") { // Local comercial
                terrenoInput.disabled = true;
                nHabitacionesInput.disabled = true;
                terrenoDiv.style.display = "none";
                habitacionesDiv.style.display = "none";
            } else if (tipoSeleccionado === "A") { // Apartamento
                terrenoInput.disabled = true;
                terrenoDiv.style.display = "none";
            } else if (tipoSeleccionado === "E") { // Edificio
                terrenoInput.disabled = true;
                terrenoDiv.style.display = "none";
            }

            // Ocultar "maletero" y "estacionamiento" si la propiedad no es "Galpón", "Townhouse" o "Penthouse"
            if (!["G", "H", "P"].includes(tipoSeleccionado)) {
                maleteroInput.disabled = true;
                estacionamientoInput.disabled = true;
                maleteroDiv.style.display = "none";
                estacionamientoDiv.style.display = "none";
            }
        }

        // Agregar un listener al evento change en el tipo de propiedad
        tipoPropiedadSelect.addEventListener("change", deshabilitarCampos);

        // Llamar a la función al cargar la página para manejar el estado inicial
        deshabilitarCampos();
    });
</script>

